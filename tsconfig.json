{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "lib": ["es6", "es7", "es2017.object", "dom"],
    "jsx": "react",
    "sourceMap": true,
    "allowJs": true,
    "listFiles": true
  },
  "exclude": [
    "node_modules"
  ],
}